{
  "name": "@pkmn/sets",
  "version": "5.0.3",
  "description": "Parsing logic for Pok√©mon Showdown sets and teams",
  "repository": "github:pkmn/ps",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "unpkg": "build/index.min.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@pkmn/types": "file:../types"
  },
  "scripts": {
