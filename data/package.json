{
  "name": "@pkmn/data",
  "version": "0.7.13",
  "description": "A forked implementation of the Pok√©mon Showdown client's data layer",
  "repository": "github:pkmn/ps",
  "license": "MIT",
  "sideEffects": false,
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "unpkg": "build/index.min.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "build"
  ],
  "dependencies": {
    "@pkmn/dex-types": "file:../dex/types"
  },
  "devDependencies": {
    "@pkmn/dex": "file:../dex",
    "@pkmn/mods": "file:../mods",
    "@pkmn/sim": "file:../sim",
    "@smogon/calc": "^0.7.0"
  },
  "scripts": {
    "lint": "eslint --cache *.ts",
    "fix": "eslint --fix *.ts",
    "compile": "tsc -p .",
    "build": "npm run compile && node ../build index.ts --minify",
    "test": "jest",
    "pretest": "npm run build",
    "posttest": "npm run lint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "node_modules/",
      "build/"
    ],
    "moduleNameMapper": {
      "^@smogon/calc/(.*)$": "<rootDir>/node_modules/@smogon/calc/dist/$1",
      "^@pkmn/mods/(.*)$": "<rootDir>/node_modules/@pkmn/mods/build/$1"
    }
  }
}
