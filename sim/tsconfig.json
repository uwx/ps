{
  "extends": "../tsconfig-base.json",
  "compilerOptions": {
    "allowJs": true,
    "allowUnusedLabels": false,
    "checkJs": true,
    "lib": ["es2020"],
    "target": "es2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": false,
    "noImplicitReturns": false,
    "tsBuildInfoFile": ".tsbuildinfo",
    
    "rootDir": ".",
    "outDir": "build",
  },
  "exclude": ["test", "build"]
}
